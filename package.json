    "Joachim Karlsson"
  ],
  "license": "MIT",
  "devDependencies": {
    "http-server": "^0.6.1",
    "karma": "~0.12.6",
    "karma-jasmine": "~0.2.0",
    "karma-chrome-launcher": "~0.1.3",
    "karma-firefox-launcher": "~0.1.3",
    "gulp": "~3.6.0",
    "gulp-ngmin": "~0.2.0",
    "gulp-uglify": "~0.2.1",
    "gulp-jshint": "~1.5.3",
    "gulp-concat": "~2.2.0",
    "gulp-rename": "~1.2.0",
    "gulp-clean": "~0.2.4",
    "gulp-karma": "0.0.4",
    "protractor": "~0.22.0",
    "jasmine-given": "~2.6.0"
  }
}
